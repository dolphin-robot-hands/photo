

<% pic.each do |photo| %>
      <div class="col-sm-4 align-self-center">
          <div class="portfolio-item align-self-center">
              <a class="img-portfolio img-responsive" href = "<%= photo.file_url %>"
                  data-lightbox="example-set" data-title=<%= %>>
                  <img class="img-portfolio img-responsive align-self-center" src="<%= photo.file_url(:thumb) %>"  alt="" /></a>
          </div>
      </div>
<% end %>
